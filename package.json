{
  "name": "walkthrough-js",
  "version": "0.0.1",
  "description": "A javascript library for easy walkthrough and site tour / tutorial",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --port 9000 --inline --progress --profile --colors --watch --content-base  --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "build.prod": "webpack --config webpack.config.js -p",
    "test": "karma start karma.config.js"
  },
  "keywords": [
    "walkthrough",
    "tour",
    "tutorial",
    "javascript"
  ],
  "author": "Ran Buchnik",
  "license": "MIT",
  "dependencies": {
    "rxjs": "6.5.2"
  },
  "devDependencies": {
    "@types/jasmine": "3.3.12",
    "@types/node": "12.0.2",
    "awesome-typescript-loader": "5.2.0",
    "jasmine-core": "3.1.0",
    "karma": "2.0.4",
    "karma-jasmine": "1.1.2",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-webpack": "3.0.0",
    "source-map-loader": "0.2.3",
    "tslint": "5.10.0",
    "tslint-loader": "3.6.0",
    "typescript": "2.8.3",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4"
  }
}
