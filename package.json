{
  "name": "walkthrough-js",
  "version": "0.0.9",
  "description": "A javascript library for easy walkthrough and site tour / tutorial",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ranbuch/walkthrough-js.git"
  },
  "scripts": {
    "start": "webpack-dev-server --port 9000 --inline --progress --profile --colors --watch --content-base  --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "build.prod": "webpack --config webpack.config.js -p",
    "test": "karma start karma.config.js",
    "build.esnext": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build.all": "npm run build.prod && npm run build.esnext"
  },
  "keywords": [
    "walkthrough",
    "tour",
    "tutorial",
    "step-by-step",
    "how-to",
    "guide",
    "hint",
    "intro",
    "vanilla",
    "ESNext",
    "javascript"
  ],
  "author": "Ran Buchnik",
  "license": "MIT",
  "dependencies": {
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jasmine": "3.3.12",
    "@types/node": "12.0.2",
    "awesome-typescript-loader": "5.2.0",
    "jasmine-core": "3.1.0",
    "karma": "^6.3.14",
    "karma-jasmine": "1.1.2",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-webpack": "3.0.0",
    "source-map-loader": "0.2.3",
    "tslint": "5.17.0",
    "tslint-loader": "3.6.0",
    "typescript": "3.5.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
